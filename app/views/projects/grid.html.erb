<div class="span-14 prepend-8 append-2">
  <% @projects.each do |project| %>
    <% if project.description %>
      <%= link_to image_tag("#{project.thumbnail}", :size => "75x75", :alt => "${project.description}"), project %>
      <% i = 0 %>
      <% i = i + 1 %>
      <% if (i.divmod(5) == 0) || (i.divmod(3) ==0) %>
        <br />
      <% end %>
    <% end %>
  <% end %>
</div>
